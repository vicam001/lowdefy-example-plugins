{
    "name": "plugin-algolia",
    "version": "4.0.0-rc.10",
    "license": "Apache-2.0",
    "description": "Algolia Lowdefy blocks.",
    "homepage": "https://lowdefy.com",
    "keywords": [
      "lowdefy",
      "lowdefy blocks",
      "lowdefy plugin",
      "algolia",
      "algolia docsearch"
    ],
    "bugs": {
      "url": "https://github.com/lowdefy/lowdefy/issues"
    },
    "contributors": [
      {
        "name": "Sam Tolmay",
        "url": "https://github.com/SamTolmay"
      },
      {
        "name": "Gerrie van Wyk",
        "url": "https://github.com/Gervwyk"
      }
    ],
    "repository": {
      "type": "git",
      "url": "https://github.com/lowdefy/lowdefy.git"
    },
    "type": "module",
    "exports": {
      "./*": "./dist/*",
      "./blocks": "./dist/blocks.js",
      "./types": "./dist/types.js"
    },
    "files": [
